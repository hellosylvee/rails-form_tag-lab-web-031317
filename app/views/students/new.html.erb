<form action="<%= students_path %>" method="post">
  <h1>Student Form</h1>
  <input id="first_name" type="text" name="student[first_name]">
  <input id="last_name" type="text" name="student[last_name]">

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="submit" value="Submit Student">
</form>
